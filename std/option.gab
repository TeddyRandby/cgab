#
# An optional.
#

let SOME, NONE = $some, $none

#
# Some
#

def Some { [SOME] }

def some[]
    { [SOME]: self }
end

def print[Some]
    'Some({self[SOME]})':print
end

def unwrap[Some]
    self[$some]
end

def unwrap_or[Some](alternative)
    self[SOME]
end

#
# None
#

def none[]
  $none
end

def print[NONE]
  'None()':print
end

def unwrap[NONE]
    panic('Tried to unwrap none')
end

def unwrap_or[NONE](alternative)
    alternative
end
