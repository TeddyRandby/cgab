:use 'num'

def nums.int_between[](a, b)
  :float.between(a, b):floor
end

def _nums.from[nil](n)
  { from = n }
end

def _nums.from[.gab.number](n)
  { from = n, to = self }
end

def nums.from[.gab.number](src)
  src:_num.from(self)
end

def _nums.to[nil](n)
  { to = n }
end

def _nums.to[.gab.number](n)
  { from = self, to = n }
end

def nums.to[.gab.number](dest)
  dest:_num.to(self)
end
