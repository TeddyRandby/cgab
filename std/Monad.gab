# @returns The error object

def err[] { err = self } end

def some?[] true end

def ok?[] true end

def value![] self end

def value_or[](alt) self end

def value_else[](cb) self end

impl nil
  def some? false end

  def value! 'Called value! on nil':panic end

  def value_or(alt) alt end

  def value_else(cb) cb() end
end

def Error { err }

impl Error
  def ok? false end

  def value! self.err:panic end

  def value_or(alt) alt end

  def value_else(cb) cb() end
end
