def Ok  { ok  }
def Err { err }

def ok[]
  { ok: self }
end

def err[]
  { err: self }
end

def print[Ok]
  'Ok({self.ok})':print
end

def print[Err]
  'Err({self.err})':print
end

def unwrap[Ok]
  self.ok
end

def unwrap[Err]
  self:print!
end

def unwrap_or[Ok](alternative)
  self.ok
end

def unwrap_or[Err](alternative)
  alternative
end
