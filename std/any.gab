def is?[](other); ?self == other end

def is_b?[]        ; false end
def is_b?[.Boolean]; true end

def is_n?[]       ; false end
def is_n?[.Number]; true end

def is_s?[]       ; false end
def is_s?[.String]; true end

def to_b[]; self end

def deferto[](..messages, target)
  i = 0

  loop
    def (messages[i])[self](..input); self:(target)(..input) end
    i = i + 1
  until i == messages:len end
end
