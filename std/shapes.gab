'enum' :use
shapes.t = 'shp' :use

\shapes.next? :def.iter.next! ([shapes.t], \next?)

keys[.Shape]
    key = self:next

    loop
      yield key
      key = self:next(key)
    until key == nil end
end

def values[.Shape]
    key = self:next

    loop
      yield key
      key = self:next(key)
    until key == nil end
end
