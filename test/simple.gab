def Solver { input, rowlen, begin, length, is_part? }

def check[Solver](delta_y)
  row, col = self:coord
  row = row + delta_y 

  '{col},{row}:checking':print

  row > 0 and row * self:rowlen <= self:input:len then
    col > 0 and col <= self:rowlen then
      self:is_part? = self:input:at(row * self:rowlen + col):is_symbol?
    end
  end
end


:use .dis

&:check:dis(Solver)
