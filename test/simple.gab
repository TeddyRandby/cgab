'core' :use
'records':use
'numbers' :use

record = [1,2,3]
sum = record:map(\values, 0, \+, \++)
'map record with \++, reduce with \+: {sum}':print

td = .td:compose (
  .td:map do x; '{x}' end,
  .td:interpose ',',
)

record:transduce(\pairs, '', \+, td):print

10:tap(\upto, .nil, \self, \print)

iter = .gab.iterator (record, \values, 0, \+)
iter:print
iter:iter.next:print
iter:iter.next:print
iter:iter.next:print
iter:iter.next:print
