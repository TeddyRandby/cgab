'Range':require

test = do
    for _ in :range(30) # Puts a suspense on the stack
        for i, j in :range(30) # Puts a suspense on the stack
            yield i, j # Creates a suspense, which references these two suspenses
                        # And the block.
        end
    end
end

for i, j in test()
    '{i}, {j}':print
end
