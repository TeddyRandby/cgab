def describe(name, cb)
  !print(name)
  cb()
end
!describe = describe

!print(!describe)

def it(name, cb)
  !print(cb)
  cb()
end
!it = it

def check(result, msg)
  if not result
    !print(msg)
  end
end

def assert {
  def equalTo(a, b)
    check(a == b, 'Expected {a} to be equal to {b}')
  end
}
!assert = assert 
