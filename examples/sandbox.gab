def odd(num)
  match num?
    'number' => match num % 2
        1 => return true
        0 => return false
        ? => return null
    ? => return null, 'expected a number'
end

def times(i)
  let v = 0
  do ||
    v < i and (v = v + 1)
  end
end

for i in 5:times()
  !print('{i} is odd: {i:odd()}')
end
