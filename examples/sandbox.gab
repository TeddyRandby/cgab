def each(i)
    let v = -1

    do ||
        return i[(v = v + 1)], v
    end
end

def join(arr)
    let str = ""

    for s in arr:each()
        str = '{str}{s}'
    end

    str
end

let hello = 'hello'
(hello == 'hello'):!print()

[hello ' ' 'world' '!']:join():!print()

(hello == 'hello'):!print()
