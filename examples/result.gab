# Here is an example implementation of Rust's Result type
def Ok { ok }

def Err { err }

def ok()
  { ok: self }
end

def err()
  { err: self }
end

def print[Ok]
    'Ok({self.ok})':print
end

def print[Err]
    'Err({self.err})':print
end

def unwrap[Ok]
  self.ok
end

def unwrap[Err]
  self.err:print
  null!
end

def unwrap_or[Ok]
  self.ok
end

def unwrap_or[Err](alternative)
  alternative
end

def unwrap_orelse[Ok]
    self.ok
end

def unwrap_orelse[Err](cb)
   :(cb) 
end
