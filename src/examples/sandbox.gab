let ss = !require("../src/examples/ssgen.gab")

let node, div, h = ss.Node, ss.div, ss.h

let page = node.new()

page->node.add(h('Hello World', 2))

page->node.add(div('This is generated'))

let sub_page = node.new()->node.add(h('Sub Header', 4))

page->node.add(sub_page)

page->node.add(div('New Div'))

let output = page->node.build()

!write("../src/examples/output.html", output)
